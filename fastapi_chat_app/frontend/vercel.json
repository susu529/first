{
  "version": 2,
  "name": "sweetheart-frontend",
  "public": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://YOUR_HEROKU_APP.herokuapp.com/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/js/runtime-config.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}


